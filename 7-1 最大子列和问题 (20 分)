#include<stdio.h>

int MaxSubseqsum2(int A[], int N);

int main(){
    int n;
    int numseq[100000];
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d",&numseq[i]);
    }
    printf("%d\n",MaxSubseqsum2(numseq,n));
    return 0;
}

int MaxSubseqsum2(int A[], int N){
    int i, j;
    int thisSum, MaxSum = 0;
    for ( i = 0; i < N; i++)
    {
        thisSum = 0;
        for ( j = i; j < N; j++)
        {
            thisSum+=A[j];
        
        if (thisSum > MaxSum)
        {
            MaxSum = thisSum;
        } 
        }
       
    }
    return MaxSum;
}
